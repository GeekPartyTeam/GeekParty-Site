{% extends 'GeekPartyBundle::parent.html.twig' %}

{% block main %}
    <div class="container">
        <div class="row">
            <ul>
                {% for user in users %}
                    <li>
                        <a href="{{ path('geek_party_user_show', {id: user.id}) }}">{{ user.fullname }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}

{% block title %} {{ parent() }} :: Список пользователей {% endblock %}

